{
  "name": "search",
  "version": "0.1.0",
  "private": true,
  "homepage": "/search",
  "dependencies": {
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "rewire": "^4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -f -r target && mkdir -p target/search",
    "build": "node ./scripts/build-non-split.js",
    "postbuild": "mv build/* target/search/ && node ./scripts/createComponentManifest.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
